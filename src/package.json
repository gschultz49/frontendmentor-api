    "test-local": "cypress run --spec 'e2e/cypress/integration/*.ts' --env baseUrl='http://localhost:3000/api' --project e2e/",
    "test-prod": "cypress run --spec 'e2e/cypress/integration/*.ts' --env baseUrl='https://frontendmentor-api.now.sh/api' --project e2e/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@now/node": "^1.5.0",
    "isomorphic-fetch": "^2.2.1",
    "js-yaml": "^3.13.1",
    "next": "^9.3.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "swr": "^0.2.0"
  },
  "devDependencies": {
    "@types/node": "^13.9.5",
    "@types/react": "^16.9.26",
    "hoist-non-react-statics": "^3.3.2",
    "postcss-preset-env": "^6.7.0",
    "tailwindcss": "^1.2.0",
    "typescript": "^3.8.3",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.1",
    "prettier": "^2.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": [
        "lint-staged",
        "npm run test-local"
      ]
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  }
